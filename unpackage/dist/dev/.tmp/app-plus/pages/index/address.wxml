<view class="address-management"><block wx:if="{{addressList.length>0}}"><view class="address-management-list"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="address-management-list-item flex-center-between"><view><view><text>{{item.name}}</text><text>{{item.mobile}}</text><text class="default">{{item.isDefault?'默认':''}}</text></view><view class="address">{{item.address}}</view></view><text data-event-opts="{{[['tap',[['pageTo',['$event']]]]]}}" class="iconfont iconbianji" bindtap="__e"></text></view></block></view></block><block wx:else><view class="no-data">暂无收货地址哦~</view></block><view class="address-add"><button data-event-opts="{{[['tap',[['pageTo',['$event']]]]]}}" class="address-add-btn" bindtap="__e">+ 新建地址</button></view></view>