.address-management-top {
  height: 10rpx;
  width: 100%;
  background: url(data:image/png;base64,) no-repeat 100%/100%;
}
.address-management-list {
  padding: 0 30rpx;
  border-bottom: 1px solid #e2dcf2;
}
.address-management-list-item {
    height: 168rpx;
    border-bottom: 1px solid #e2dcf2;
}
.address-management-list-item:last-child {
      border-bottom: none;
}
.address-management-list-item .default {
      display: inline-block;
      padding: 0 10rpx;
      margin: 0 20rpx;
      font-size: 26rpx;
      color: #fff;
      -webkit-border-radius: 8rpx;
              border-radius: 8rpx;
      background-image: -webkit-linear-gradient(135deg, #00b5c4 0%, #52c7d1 100%), -webkit-linear-gradient(#52c7d1, #52c7d1);
      background-image: -o-linear-gradient(135deg, #00b5c4 0%, #52c7d1 100%), -o-linear-gradient(#52c7d1, #52c7d1);
      background-image: linear-gradient(-45deg, #00b5c4 0%, #52c7d1 100%), linear-gradient(#52c7d1, #52c7d1);
}
.address-management-list-item .address {
      font-size: 26rpx;
}
.address-management .address-add {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.address-management .address-add-btn {
    margin: 38rpx auto;
    width: 559rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 30rpx;
