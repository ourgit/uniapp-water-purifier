<view class="address-management"><view class="address-management-top"></view><block wx:if="{{addressList.length>0}}"><view class="address-management-list"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="address-management-list-item flex-center-between"><view><view><text>{{item.name}}</text><text>{{item.mobile}}</text><text class="default">{{item.isDefault?'默认':''}}</text></view><view class="address">{{item.address}}</view></view><uni-icon vue-id="{{'1-'+index}}" type="compose" size="26" data-event-opts="{{[['^click',[['pageTo']]]]}}" bind:click="__e" bind:__l="__l"></uni-icon></view></block></view></block><block wx:else><view class="no-data">暂无收货地址哦~</view></block><view class="address-add"><button data-event-opts="{{[['tap',[['pageTo',['$event']]]]]}}" class="address-add-btn" bindtap="__e"><uni-icon vue-id="2" type="plusempty" color="#fff" size="20" bind:__l="__l"></uni-icon><text>新建地址</text></button></view></view>